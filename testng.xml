<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test thread-count="5" name="ChromeTest">
 <parameter name="node" value="local"> </parameter>
      <groups>
  		<run>
  			<include name="Regression"/>
  		</run>
  	 </groups>
  	 
    <classes>
         <class name="com.flipkart.testng.FlipkartTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="FirefoxTest">
 <parameter name="node" value="external"> </parameter>
      <groups>
  		<run>
  			<include name="Regression"/>
  		</run>
  	 </groups>
  	 
    <classes>
         <class name="com.flipkart.testng.FlipkartTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
